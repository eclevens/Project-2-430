    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^8.1.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.32.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.5",
    "nodemon": "^3.1.10",
    "rimraf": "^6.1.2",
    "style-loader": "^4.0.0",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
